.stweb__mtv-banner-hero
    picture.picture--bg
        source(media="(min-width: 768px)", srcset="images/pages/movistar-tv/banner-movistar-tv-desktop.jpg")
        img(src="", alt="banner fibra optica", srcset="images/pages/movistar-tv/banner-movistar-tv-mobile.jpg")
    .content 
        h2.title Vive la experiencia Movistar TV
        p.text Disfruta de los mejores programas de televisión del Perú e internacionales con nuestros súper planes de cable
        .promotion 
            picture.picture--pmt 
                source(media="(min-width: 768px)", srcset="images/pages/movistar-tv/movistar-tv-promo-desktop.png")
                img(src="", alt="ahorra 40 soles por 2 meses", srcset="images/pages/movistar-tv/movistar-tv-promo-mobile.png")
.stweb__mtv-tabs-Planes
    .coverage
        .content 
            .row.no-gutters
                .col-md-auto.map
                    picture.picture
                        source(media="(min-width: 768px)", srcset="images/cobertura/map-ilustration-desktop.png")
