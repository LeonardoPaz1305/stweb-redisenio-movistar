.stweb__preplan-banner-hero
    .swiper.banner-hero__container
        .swiper-wrapper
            .swiper-slide
                .banner-hero__content
                    picture.background
                        img(src="images/pages/preplan/banner-preplan-mobile.png", alt="")
                    .content 
                        h2.title ¡Si eres Prepago afíliate a Preplan!
                        p.text Con tu primera recarga desde S/5 llévate un bono de bienvenida
                        picture.picture 
                            source(media="(min-width: 768px)", srcset="images/pages/preplan/freefire-desktop.png")
                            img(src="images/pages/preplan/freefire-mobile.png", alt="freefire", srcset="images/pages/preplan/freefire-mobile.png")
        .swiper-arrow
            .swiper-button-prev
                <svg class="icon svg--inline" focusable="false" role="img" viewBox="0 0 8 15" fill="none" xmlns="http://www.w3.org/2000/svg">
                <path d="M7 1.7334L1 7.7334L7 13.7334" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                </svg>
            .swiper-button-next
                <svg class="icon svg--inline" focusable="false" role="img" viewBox="0 0 8 15" fill="none" xmlns="http://www.w3.org/2000/svg">
                <path d="M1 13.7334L7 7.7334L1 1.7334" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                </svg>  

.stweb__preplan-tabs
    .container
        .tab-panel
            .tabs-wrapper
                ul.nav.nav-tabs#tabs
                    li.nav-item(data-tab='tab-1')
                        a(href="http://" target="_blank").nav-link Prepago
                    li.nav-item.active(data-tab='tab-2')
                        a(href="javascript:void(0)").nav-link Preplan
                    li.nav-item(data-tab='tab-3')
                        a(href="http://" target="_blank").nav-link Paquetes     
                    li.nav-item(data-tab='tab-4')
                        a(href="http://" target="_blank").nav-link Recargar                                                  
