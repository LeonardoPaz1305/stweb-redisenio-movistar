.stweb__fo-banner-hero
    picture.picture--bg
        source(media="(min-width: 768px)", srcset="images/slider/banner-fibra-optica-desktop.png")
        img(src="", alt="banner fibra optica", srcset="images/slider/banner-fibra-optica-mobile.png")
